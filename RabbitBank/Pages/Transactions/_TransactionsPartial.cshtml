@{
}

@model List<RabbitBank.Pages.ViewModels.TransactionsModel>

@foreach (var transaction in Model)
{
    <tr>
        <td>@transaction.Date</td>
        <td>@transaction.Transaction</td>
        <td>@transaction.Amount</td>
        <td>@transaction.Balance</td>
    </tr>
}